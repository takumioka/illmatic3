<%= provide(:title,session[:name]) %>   

<table>
  <thead>
    <tr>
      <th>Messagecontent</th>
      <th>Nickname</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @bulletinboards.each do |bulletinboard| %>
      <tr>
        <td><%= bulletinboard.messagecontent %></td>
        <td><%= bulletinboard.nickname %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Bulletinboard', new_bulletinboard_path %>
